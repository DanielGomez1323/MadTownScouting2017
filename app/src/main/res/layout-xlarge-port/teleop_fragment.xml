<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.pollo.madtownscouting2017.TeleopPeriod">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Gears Picked Up:"
        android:id="@+id/gearsPickedUpTextView"
        android:textSize="40sp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/teleopPeriodTextView"
        android:layout_centerHorizontal="true"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Teleoperated Period"
        android:id="@+id/teleopPeriodTextView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="50sp"/>

    <Button
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:text="-"
        android:id="@+id/decreaseGearsPickedUpButton"
        android:layout_below="@+id/gearsPickedUpTextView"
        android:layout_toLeftOf="@+id/gearsPickedUpTextView"
        android:layout_toStartOf="@+id/gearsPickedUpTextView"
        android:layout_marginTop="15dp"
        android:paddingTop="5dp"/>

    <Button
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:text="+"
        android:id="@+id/increaseGearsPickedUpButton"
        android:paddingTop="5dp"
        android:layout_below="@+id/gearsPickedUpTextView"
        android:layout_toEndOf="@+id/gearsPickedUpTextView"
        android:layout_marginTop="15dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="0"
        android:id="@+id/amountGearsPickedUpTextView"
        android:textSize="30sp"
        android:layout_centerHorizontal="true"
        android:layout_alignBottom="@+id/increaseGearsPickedUpButton"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Gears Dropped:"
        android:id="@+id/gearsDroppedTextView"
        android:textSize="40sp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/amountGearsPickedUpTextView"
        android:layout_centerHorizontal="true"/>

    <Button
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:text="-"
        android:id="@+id/decreaseGearsDroppedButton"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/gearsDroppedTextView"
        android:layout_alignLeft="@+id/decreaseGearsPickedUpButton"
        android:paddingTop="5dp"/>

    <Button
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:text="+"
        android:id="@+id/increaseGearsDroppedButton"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/gearsDroppedTextView"
        android:layout_alignLeft="@+id/increaseGearsPickedUpButton"
        android:paddingTop="5dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="0"
        android:id="@+id/amountGearsDroppedTextView"
        android:layout_alignBottom="@+id/increaseGearsDroppedButton"
        android:layout_centerHorizontal="true"
        android:textSize="30sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Gears Placed:"
        android:id="@+id/gearsPlacedTextView"
        android:layout_below="@+id/decreaseGearsDroppedButton"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textSize="40sp"/>

    <Button
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:text="-"
        android:id="@+id/decreaseGearsPlacedButton"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/gearsPlacedTextView"
        android:layout_alignRight="@+id/decreaseGearsDroppedButton"
        android:paddingTop="5dp"/>

    <Button
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:text="+"
        android:id="@+id/increaseGearsPlacedButton"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/gearsPlacedTextView"
        android:layout_alignLeft="@+id/increaseGearsDroppedButton"
        android:paddingTop="5dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="0"
        android:id="@+id/amountGearsPlacedTextView"
        android:layout_alignBottom="@+id/increaseGearsPlacedButton"
        android:layout_centerHorizontal="true"
        android:textSize="30sp"/>

    <SeekBar
        android:layout_width="400dp"
        android:layout_height="30dp"
        android:id="@+id/ballrateSeekBar"
        android:max="10"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/amountGearsPlacedTextView"
        android:layout_marginTop="15dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Ball Rate"
        android:id="@+id/ballrateTextView"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/ballrateSeekBar"
        android:textSize="30sp"
        android:layout_marginTop="0dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="0"
        android:id="@+id/zerorateTextView"
        android:layout_marginTop="0dp"
        android:layout_below="@+id/ballrateSeekBar"
        android:layout_toLeftOf="@+id/ballrateSeekBar"
        android:layout_toStartOf="@+id/ballrateSeekBar"
        android:textSize="30sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="10"
        android:id="@+id/tenrateTextView"
        android:layout_below="@+id/ballrateSeekBar"
        android:layout_toRightOf="@+id/ballrateSeekBar"
        android:layout_toEndOf="@+id/ballrateSeekBar"
        android:layout_marginTop="0dp"
        android:textSize="30sp"/>

    <SeekBar
        android:layout_width="400dp"
        android:layout_height="30dp"
        android:id="@+id/highballsmissedSeekBar"
        android:max="100"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/ballrateTextView"
        android:layout_alignRight="@id/ballrateSeekBar"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="High Missed"
        android:id="@+id/highmissedTextView"
        android:layout_below="@+id/highballsmissedSeekBar"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:textSize="30sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:textAppearanceSmall"
        android:text="0"
        android:id="@+id/zeromissedTextView"
        android:layout_below="@+id/highballsmissedSeekBar"
        android:layout_toLeftOf="@+id/highballsmissedSeekBar"
        android:layout_toStartOf="@+id/highballsmissedSeekBar"
        android:layout_marginTop="0dp"
        android:textSize="30sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:textAppearanceSmall"
        android:text="100"
        android:id="@+id/onehundredmissedTextView"
        android:layout_below="@+id/highballsmissedSeekBar"
        android:layout_toRightOf="@+id/highballsmissedSeekBar"
        android:layout_toEndOf="@+id/highballsmissedSeekBar"
        android:layout_marginTop="0dp"
        android:textSize="30sp" />

    <SeekBar
        android:layout_width="400dp"
        android:layout_height="30dp"
        android:id="@+id/lowballrateSeekBar"
        android:layout_below="@+id/highmissedTextView"
        android:layout_centerHorizontal="true"
        android:max="10"
        android:layout_marginTop="15dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Low Ball Rate"
        android:id="@+id/lowballTextView"
        android:layout_below="@+id/lowballrateSeekBar"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:textSize="30sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="0"
        android:id="@+id/lowzerorateTextView"
        android:layout_marginTop="0dp"
        android:layout_below="@+id/lowballrateSeekBar"
        android:layout_toLeftOf="@+id/lowballrateSeekBar"
        android:layout_toStartOf="@+id/lowballrateSeekBar"
        android:textSize="30sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="10"
        android:id="@+id/lowtenrateTextView"
        android:textSize="30sp"
        android:layout_marginTop="0dp"
        android:layout_below="@+id/lowballrateSeekBar"
        android:layout_toRightOf="@+id/lowballrateSeekBar"
        android:layout_toEndOf="@+id/lowballrateSeekBar" />

    <SeekBar
        android:layout_width="400dp"
        android:layout_height="30dp"
        android:id="@+id/lowballmissedSeekBar"
        android:max="100"
        android:layout_below="@+id/lowballTextView"
        android:layout_alignRight="@+id/lowballrateSeekBar"
        android:layout_marginTop="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="0"
        android:id="@+id/zerolowmissedTextView"
        android:layout_below="@+id/lowballmissedSeekBar"
        android:layout_toLeftOf="@+id/lowballmissedSeekBar"
        android:layout_toStartOf="@+id/lowballmissedSeekBar"
        android:layout_marginTop="0dp"
        android:textSize="30sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Low Missed"
        android:id="@+id/lowballmissedTextView"
        android:layout_below="@+id/lowballmissedSeekBar"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:textSize="30sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="100"
        android:id="@+id/onehundredlowmissedTextView"
        android:layout_below="@+id/lowballmissedSeekBar"
        android:layout_toRightOf="@id/lowballmissedSeekBar"
        android:layout_toEndOf="@+id/lowballmissedSeekBar"
        android:layout_marginTop="0dp"
        android:textSize="30sp" />

    <SeekBar
        android:layout_width="400dp"
        android:layout_height="30dp"
        android:id="@+id/hopperintakeSeekBar"
        android:max="100"
        android:layout_below="@+id/lowballmissedTextView"
        android:layout_alignRight="@+id/lowballmissedSeekBar"
        android:layout_marginTop="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="0"
        android:id="@+id/zerointakenTextView"
        android:layout_below="@+id/hopperintakeSeekBar"
        android:layout_toLeftOf="@+id/hopperintakeSeekBar"
        android:layout_toStartOf="@+id/hopperintakeSeekBar"
        android:layout_marginTop="0dp"
        android:textSize="30sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Hopper Intake"
        android:id="@+id/hopperintakeTextView"
        android:layout_below="@+id/hopperintakeSeekBar"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:textSize="30sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="100"
        android:id="@+id/onehundredhopperintakeTextView"
        android:layout_below="@+id/hopperintakeSeekBar"
        android:layout_toRightOf="@+id/hopperintakeSeekBar"
        android:layout_toEndOf="@+id/hopperintakeSeekBar"
        android:layout_marginTop="0dp"
        android:textSize="30sp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Successful Climb?"
        android:id="@+id/successfulclimbcheckBox"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/hopperintakeTextView"
        android:layout_centerHorizontal="true"
        android:textSize="30sp"/>

    <Chronometer
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/climbChronometer"
        android:textSize="30sp"
        android:text="00"
        android:layout_marginTop="15dp"
        android:layout_alignBottom="@+id/startclimbButton"
        android:layout_alignLeft="@+id/lowzerorateTextView"
        android:layout_alignStart="@+id/lowzerorateTextView" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Start/Stop"
        android:id="@+id/startclimbButton"
        android:layout_marginTop="15dp"
        android:textSize="30sp"
        android:layout_below="@+id/successfulclimbcheckBox"
        android:layout_centerHorizontal="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30sp"
        android:text="Reset"
        android:id="@+id/resetclimbButton"
        android:layout_marginTop="15dp"
        android:layout_alignBottom="@+id/climbChronometer"
        android:layout_toRightOf="@+id/increaseGearsPlacedButton"
        android:layout_toEndOf="@+id/increaseGearsPlacedButton" />
</RelativeLayout>
</ScrollView>